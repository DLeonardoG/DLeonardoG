---
import Layout from "../../layouts/Layout.astro";
import Contacto from "../../components/Contact.astro";
import Header from "../../components/header.astro";
import { ui } from '../../i18n/ui';
const lang = 'es';  
const menus = ui[lang].menu;
---
<Layout title="Contacto">
 <div>
   <Header menus={menus} />
    <Contacto welcomePhrase={ui[lang]['pages.contact.welcomePhrase']} name={ui[lang]['pages.contact.form.name']} phrase={ui[lang]['pages.contact.form.phrase']} button={ui[lang]['pages.contact.form.button']}/>
 </div>
</Layout>